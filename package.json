{
  "name": "testo",
  "version": "0.0.1",
  "description": "API for testo",
  "repository": {
    "url": "https://github.com/jacklinton/testo.git"
  },
  "main": "./src/app.js",
  "type": "module",
  "scripts": {
    "test": "jest --group=unit",
    "test:watch": "jest --watchAll --group=unit",
    "test:coverage": "jest --coverage --group=unit",
    "test:integration": "jest --group=integration",
    "start": "nodemon -r esm src/app.js",
    "start:prod": "node -r esm src/app.js",
    "autodocs": "swagger-jsdoc -d swaggerDef.js src/routes/api/*.js -o ./public/docs/swagger.json",
    "autodevdocs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "generate:types": "tsc -p tsconfig.build.json",
    "migrate": "knex migrate:latest && npm start"
  },
  "author": "Jack Linton",
  "license": "UNLICENSED",
  "dependencies": {
    "@apis/node-logger": "git+http:git@chapel.tjh.tju.edu/api-packages/node-logger#transpiled",
    "@apis/request-cache": "git+http:git@chapel.tjh.tju.edu/api-packages/request-cache",
    "@apis/request-id": "git+http:git@chapel.tjh.tju.edu/api-packages/request-id",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "camelcase": "^5.3.1",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.0",
    "express-winston": "^4.0.3",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "jest-runner-groups": "^2.0.1",
    "jsdoc": "^3.6.4",
    "jsdoc-to-markdown": "^5.0.3",
    "jsforce": "^1.10.0",
    "knex": "^0.21.1",
    "moment": "^2.27.0",
    "pg": "^8.2.1",
    "redis": "^2.8.0",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^3.7.0",
    "typescript": "^3.9.5",
    "uuid": "^3.4.0",
    "winston": "^3.3.2",
    "winston-daily-rotate-file": "^4.5.0",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-syntax-import-meta": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.9.1",
    "@types/mocha": "^5.2.7",
    "@types/redis": "^2.8.22",
    "@types/swagger-jsdoc": "^3.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jsdoc-babel": "^0.5.0",
    "minami": "^1.2.3",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1"
  },
  "engines": {
    "node": ">=12.14.0",
    "npm": ">=6.12.0",
    "nodemon": ">= 1.19.4"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/*.js",
      "src/**/*.js",
      "src/**/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "runner": "groups"
  }
}

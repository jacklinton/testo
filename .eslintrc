{
  "extends": ["airbnb", "prettier", "plugin:jest/recommended"],
  "plugins": ["jest", "prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-restricted-syntax": "off",
    "no-continue": "off",
    "import/extensions": "off",
    "prefer-const": "warn"
  },
  "env": {
    "jest/globals": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module",
    "ecmaFeatures": {
      "globalReturn": true,
      "impliedStrict": true
    }
  }
}
